{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "date": {
      "type": "string",
      "required": true
    },
    "location": {
      "type": "string",
      "required": true
    },
    "ageGroup": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": ["Neu", "Bald voll", "Ausgebucht"],
      "required": true
    },
    "camp": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::camp.camp",
      "inversedBy": "events"
    }
  }
}
